{
  "_version": "1.0.0",
  "sap.app": {
    "_version": "1.0.0",
    "id": "ch.berncon.generator.ui5",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "data": {
        "uri": "data/5w1hExtraction.json",
        "type": "JSON",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "_version": "1.42.7",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_hcb",
      "sap_bluecrystal"
    ]
  },
  "sap.ui5": {
    "_version": "1.42.7",
    "rootView": "ch.berncon.generator.ui5.view.App",
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "dependencies": {
      "minUI5Version": "1.42.7",
      "libs": {
        "sap.ui.core": {},
        "sap.ui.unified": {},
        "sap.ui.layout": {},
        "sap.m": {},
        "sap.uxap": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ch.berncon.generator.ui5.i18n.i18n"
        }
      },
      "data": {
        "dataSource": "data"
      }
    },
    "resources": {
      "css": {
        "uri": "css/style.css"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "ch.berncon.generator.ui5.view",
        "controlId": "app",
        "controlAggregation": "pages"
      },
      "routes": [
        {
          "pattern": "",
          "name": "main",
          "target": "main"
        },
        {
          "pattern": "newsDetail/{id}/",
          "name": "newsDetail",
          "target": "newsDetail"
        }
      ],
      "targets": {
        "main": {
          "viewName": "Main",
          "viewLevel": 1
        },
        "newsDetail": {
          "viewName": "NewsDetail",
          "viewLevel": 2
        }
       
      }
    }
  }
}
